<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="customerRepository"
          class="com.kariqu.suppliercenter.repository.impl.SupplierRepositoryImpl">
        <property name="sqlMapClient" ref="sqlMapClient"/>
    </bean>

    <bean id="productStorageRepository"
          class="com.kariqu.suppliercenter.repository.impl.ProductStorageRepositoryImpl">
        <property name="sqlMapClient" ref="sqlMapClient"/>
    </bean>

    <bean id="brandRepository"
          class="com.kariqu.suppliercenter.repository.impl.BrandRepositoryImpl">
        <property name="sqlMapClient" ref="sqlMapClient"/>
    </bean>

</beans>